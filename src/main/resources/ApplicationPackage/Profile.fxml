<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="301.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerPackage.ProfileController">
   <children>
      <Button fx:id="logout_button" layoutX="120.0" layoutY="333.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out" textFill="#66228f">
         <font>
            <Font name="Tahoma Bold" size="13.0" />
         </font></Button>
      <Label fx:id="name_label" layoutX="170.0" layoutY="175.0" text="Label">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Label fx:id="surname_label" layoutX="170.0" layoutY="220.0" text="Label">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="user_label" layoutX="170.0" layoutY="133.0" text="Label">
         <font>
            <Font name="Tahoma" size="16.0" />
         </font>
      </Label>
      <Button fx:id="heart_button" layoutX="30.0" layoutY="28.0" mnemonicParsing="false" onAction="#onHeartButtonClick">
         <graphic>
            <ImageView fitHeight="150.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/heart.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="44.0" layoutY="133.0" text="Username:" textFill="#66228f">
         <font>
            <Font name="Tahoma Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="175.0" text="First Name:" textFill="#66228f">
         <font>
            <Font name="Tahoma Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="220.0" text="Second name:" textFill="#66228f">
         <font>
            <Font name="Tahoma Bold" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
